{
  "name": "zellim-conference",
  "version": "1.0.0",
  "description": "zellim desktop conference app",
  "main": "main.js",
  "productName": "zellim conference",
  "protocols": {
    "name": "zellim-conference",
    "schemes": [
      "zellim-conference"
    ]
  },
  "scripts": {
    "start": "webpack --config ./webpack.main.js --mode development && concurrently \"npm:watch\" \"electron ./build/main.js\"",
    "start:electron": "webpack --config ./webpack.main.js --mode development && concurrently \"electron ./build/main.js\"",
    "clean": "rm -rf build dist",
    "lint": "eslint . && flow",
    "build:render": "webpack --config ./webpack.renderer.js --mode production",
    "build": "webpack --config ./webpack.main.js --mode production && webpack --config ./webpack.renderer.js --mode production",
    "dist": "npm run build && electron-builder",
    "dist:portable": "npm run dist portable",
    "postinstall": "patch-package && electron-builder install-app-deps",
    "watch": "webpack --config ./webpack.renderer.js --mode development --watch --watch-poll",
    "generate-v8-snapshots": "node ./tools/create-v8-snapshots.js",
    "copy-v8-snapshots": "node tools/copy-snapshot-bin-to-electron.js"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "build": {
    "appId": "com.zellim.desktop.conference",
    "productName": "Zellim Conference",
    "generateUpdatesFilesForAllChannels": false,
    "asarUnpack": [
      "app",
      "node_modules",
      "build"
    ],
    "files": [
      "**/*",
      "app",
      "meet",
      "resources",
      "node_modules",
      "!dist"
    ],
    "mac": {
      "artifactName": "zellim-conference.${ext}",
      "target": [
        {
          "arch": "universal",
          "target": "dmg"
        },
        {
          "arch": "universal",
          "target": "zip"
        }
      ],
      "category": "public.app-category.video",
      "darkModeSupport": true,
      "hardenedRuntime": true,
      "entitlements": "entitlements.mac.plist",
      "entitlementsInherit": "entitlements.mac.plist",
      "asarUnpack": "**/*.node",
      "extendInfo": {
        "NSCameraUsageDescription": "Zellim conference requires access to your camera in order to make video-calls.",
        "NSMicrophoneUsageDescription": "Zellim conference requires access to your microphone in order to make calls (audio/video).",
        "LSMultipleInstancesProhibited": true
      }
    },
    "mas": {
      "entitlements": "resources/entitlements.mas.plist",
      "entitlementsInherit": "resources/entitlements.mas.inherit.plist",
      "hardenedRuntime": false
    },
    "win": {
      "artifactName": "zellim-conference.${ext}",
      "target": [
        {
          "arch": [
            "ia32",
            "x64"
          ],
          "target": "portable"
        }
      ]
    },
    "nsis": {
      "oneClick": true,
      "perMachine": true,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": false,
      "runAfterFinish": false
    },
    "directories": {
      "buildResources": "resources"
    }
  },
  "keywords": [
    "zellim",
    "conference",
    "productivity",
    "video",
    "audio"
  ],
  "author": "Simon Malave <semg20@gmail.com>",
  "readmeFilename": "README.md",
  "license": "Apache-2.0",
  "dependencies": {
    "babel-preset-react": "^6.24.1",
    "bootstrap": "^5.1.3",
    "crypto": "^1.0.1",
    "electron-debug": "^3.2.0",
    "electron-reload": "^1.5.0",
    "electron-window-state": "^5.0.3",
    "fs": "^0.0.1-security",
    "fs-extra": "10.0.0",
    "jitsi-meet-electron-utils": "github:jitsi/jitsi-meet-electron-utils#64477b82008c25b203903568ec11bdac1a284b06",
    "mobx": "^6.3.5",
    "mobx-localstorage": "^1.2.0",
    "os-name": "^5.0.0",
    "react-router-dom": "^5.3.0",
    "react-toast-notifications": "^2.5.1",
    "react-toastify": "^8.0.3"
  },
  "devDependencies": {
    "@atlaskit/button": "^10.1.3",
    "@atlaskit/css-reset": "^3.0.8",
    "@atlaskit/droplist": "^7.0.19",
    "@atlaskit/field-text": "^7.1.0",
    "@atlaskit/icon": "^15.0.3",
    "@atlaskit/navigation": "^33.3.10",
    "@atlaskit/onboarding": "^6.2.0",
    "@atlaskit/page": "^8.0.12",
    "@atlaskit/panel": "^0.3.5",
    "@atlaskit/spinner": "^9.0.13",
    "@atlaskit/theme": "^7.0.5",
    "@atlaskit/toggle": "^5.0.15",
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "7.8.3",
    "@babel/plugin-transform-flow-strip-types": "7.9.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-flow": "7.9.0",
    "@babel/preset-react": "7.9.4",
    "@jitsi/js-utils": "^1.0.6",
    "@svgr/webpack": "5.4.0",
    "babel-eslint": "10.0.3",
    "babel-loader": "^8.2.3",
    "concurrently": "5.1.0",
    "css-loader": "3.5.0",
    "electron": "13.5.2",
    "electron-builder": "22.11.11",
    "electron-context-menu": "^2.5.0",
    "electron-is-dev": "^1.2.0",
    "electron-log": "^4.3.2",
    "electron-react-devtools": "0.5.3",
    "electron-store": "^5.2.0",
    "electron-updater": "^4.4.3",
    "eslint": "6.5.1",
    "eslint-config-jitsi": "github:jitsi/eslint-config-jitsi#1.0.2",
    "eslint-plugin-flowtype": "4.7.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsdoc": "22.1.0",
    "eslint-plugin-react": "7.19.0",
    "file-loader": "6.0.0",
    "flow-bin": "0.109.0",
    "history": "^4.10.1",
    "html-webpack-plugin": "4.0.4",
    "i18next": "^19.9.2",
    "moment": "^2.29.1",
    "mousetrap": "^1.6.5",
    "patch-package": "6.2.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "source-map-support": "^0.5.19",
    "style-loader": "1.1.3",
    "styled-components": "^3.4.10",
    "webpack": "4.42.1",
    "webpack-cli": "3.3.11"
  }
}
